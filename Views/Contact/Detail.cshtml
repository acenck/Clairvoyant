@{
}
@{ var contact = ViewBag.contactToDisplay;}
<div class="container shadow">


<div class="text-center">
    <h1>Contact Initials HERE</h1>
    <h3>@ViewBag.contactFullName</h3>
</div>

<div class="text-md-center">
    <table class="table table-bordered">
        <tr>
            <th>Phone</th>
            <td>@contact.Phone</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>@contact.Email</td>
        </tr>
    </table>

</div>

</div>